<%
/**
 * @member {Array} champions
 */

%>

<div class="page-header">
  <h1>Персонажи</h1>
  <a href="<%= sails.getRoute('champion', 'fill', 'admin') %>" class="btn btn-primary right"> Загрузить персанажей заново</a>
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Имя</th>
      <th>Riot id</th>
      <th>Картинка</th>
    </tr>
  </thead>
  <tbody>
    <% champions.forEach(function(item, index) { %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.riotId %></td>
        <td><%= item.image %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
